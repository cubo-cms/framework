{
  "/": {
    "regex": "^\\/$",
    "controller": "Document"
  },
  "/{controller}": {
    "regex": "^\\/([^\\/:]+)$",
    "controller": "{controller}"
  },
  "/{controller}:{id}": {
    "regex": "^\\/([^\\/:]+):([^\\/:]+)$",
    "controller": "{controller}",
    "id": "{id}"
  },
  "/{controller}/{filter}:{id}": {
    "regex": "^\\/([^\\/:]+)\\/([^\\/:]+):([^\\/:]+)$",
    "controller": "{controller}",
    "filter": "{filter}",
    "id": "{id}"
  },
  "/api/{controller}": {
    "regex": "^\\/api/([^\\/:]+)$",
    "controller": "{controller}",
    "format": "json"
  },
  "/api/{controller}:{id}": {
    "regex": "^\\/api/([^\\/:]+):([^\\/:]+)$",
    "controller": "{controller}",
    "id": "{id}",
    "format": "json"
  }
}
