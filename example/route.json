{
  "/": {
    "regex": "^\\/$",
    "dataType": "Document"
  },
  "/{dataType}": {
    "regex": "^\\/([^\\/:]+)$",
    "dataType": "{dataType}"
  },
  "/{dataType}:{id}": {
    "regex": "^\\/([^\\/:]+):([^\\/:]+)$",
    "dataType": "{dataType}",
    "id": "{id}"
  },
  "/{dataType}/{filter}:{id}": {
    "regex": "^\\/([^\\/:]+)\\/([^\\/:]+):([^\\/:]+)$",
    "dataType": "{dataType}",
    "filter": "{filter}",
    "id": "{id}"
  },
  "/api/{dataType}": {
    "regex": "^\\/api/([^\\/:]+)$",
    "dataType": "{dataType}",
    "format": "json"
  },
  "/api/{dataType}:{id}": {
    "regex": "^\\/api/([^\\/:]+):([^\\/:]+)$",
    "dataType": "{dataType}",
    "id": "{id}",
    "format": "json"
  }
}
